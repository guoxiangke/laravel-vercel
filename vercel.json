{
    "version": 2,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.6.0" },
        "api/assets.php": { "runtime": "vercel-php@0.6.0" }
    },
    "routes": [
        {
            "src": "/build/assets/(.*)",
            "dest": "/api/assets.php?assets=$1"
        }, 
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}